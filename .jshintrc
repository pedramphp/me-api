{
  // Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
  // Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
  // Documentation: http://www.jshint.com/docs/
  "browser": true,
  "esnext": true,
  "globals": {
    "jasmine": false,
    "spyOn": false,
    "it": false,
    "console": false,
    "describe": false,
    "expect": false,
    "beforeEach": false,
    "waits": false,
    "waitsFor": false,
    "runs": false,
    "require": false,
    "define": false

  },
  "globalstrict": true,
  "quotmark": false, // double quote and single quote
  "smarttabs": true,
  "trailing": true,
  "undef": true,
  "unused": false,
  "node": true,
  "newcap": false
}